# .devcontainer/devcontainer.json
{
  "name": "AL2023 Dev",
  "image": "al2023-dev",
  "mounts": [
    "source=${localWorkspaceFolder},target=/workspace,type=bind"
  ],
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer",
        "ms-python.python",
        "ms-azuretools.vscode-docker"
      ]
    }
  }
}
